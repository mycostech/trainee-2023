name: dockercompose5786276324169430547
services:
  todoapi:
    build:
      context: D:\self-learning\Mycos\ToDoAPI
      dockerfile: ToDoAPI/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: todoapi
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    volumes:
    - type: bind
      source: C:\Users\ACER\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\ACER\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
  training-database:
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_SA_PASSWORD: Passw1234
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      default: null
    ports:
    - mode: ingress
      target: 1433
      published: "1433"
      protocol: tcp
    restart: unless-stopped
    volumes:
    - type: volume
      source: data
      target: /var/opt/mssql
      volume: {}
networks:
  default:
    name: dockercompose5786276324169430547_default
volumes:
  data:
    name: dockercompose5786276324169430547_data