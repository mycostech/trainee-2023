name: dockercompose15640093841121316071
services:
  todoapi:
    build:
      context: D:\TrainingAPI\ToDoAPI
      dockerfile: ToDoAPI/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Docker
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: todoapi
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      protocol: tcp
    - mode: ingress
      target: 443
      protocol: tcp
    volumes:
    - type: bind
      source: C:\Users\phorn\AppData\Roaming/Microsoft/UserSecrets
      target: /root/.microsoft/usersecrets
      read_only: true
      bind:
        create_host_path: true
    - type: bind
      source: C:\Users\phorn\AppData\Roaming/ASP.NET/Https
      target: /root/.aspnet/https
      read_only: true
      bind:
        create_host_path: true
  training-database:
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_SA_PASSWORD: '!Ntern2023'
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      default: null
    ports:
    - mode: ingress
      target: 1433
      published: "1433"
      protocol: tcp
    restart: unless-stopped
    volumes:
    - type: volume
      source: data
      target: /var/opt/mssql
      volume: {}
networks:
  default:
    name: dockercompose15640093841121316071_default
volumes:
  data:
    name: dockercompose15640093841121316071_data